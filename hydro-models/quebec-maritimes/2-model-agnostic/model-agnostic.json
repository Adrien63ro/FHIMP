{
  "exec": {
    "met": "/path/to/github-repos/datatool/extract-dataset.sh",
    "gis": "/path/to/github-repos/gistool/extract-gis.sh",
    "remap": "easymore cli"
  },

  "args": {
    "met": [{
      "dataset": "RDRS",
      "dataset-dir": "/project/rpp-kshook/Model_Output/RDRSv2.1/",
      "variable": [
        "RDRS_v2.1_P_P0_SFC",
        "RDRS_v2.1_P_HU_1.5m",
        "RDRS_v2.1_P_TT_1.5m",
        "RDRS_v2.1_P_UVC_10m",
        "RDRS_v2.1_P_PR0_SFC",
        "RDRS_v2.1_P_FB_SFC",
        "RDRS_v2.1_P_FI_SFC"
      ],
      "output-dir": "/path/to/scratch/qc-maritimes/datatool_outputs",
      "start-date": "1980-01-01T13:00:00",
      "end-date": "1980-12-31T12:00:00",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "/path/to/1-geofabric/merit-basins-qc-maritimes-boundaries/region_catchments.shp",
      "model": "",
      "ensemble": "",
      "prefix": "qc_maritimes_",
      "email": "your-email@ulaval.ca",
      "_flags": [
        "submit-job",
        "parsable"
      ]
    }],

    "gis": [{
      "dataset": "landsat",
      "dataset-dir": "/project/rpp-kshook/Model_Output/Landsat/",
      "variable": "land-cover",
      "start-date": "2020",
      "end-date": "2020",
      "output-dir": "/path/to/scratch/qc-maritimes/gistool_outputs/landsat",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "/path/to/1-geofabric/merit-basins-qc-maritimes-boundaries/region_catchments.shp",
      "print-geotiff": "true",
      "stat": [
        "frac",
        "coords"
      ],
      "quantile": "",
      "prefix": "qc_maritimes_",
      "email": "kasra.keshavarz1@ucalgary.ca",
      "_flags": [
	"include-na",
        "submit-job",
        "parsable"
      ]
    },

    {
      "dataset": "soil_class",
      "dataset-dir": "/project/rpp-kshook/Model_Output/soil_classes/",
      "variable": "soil_classes",
      "start-date": "",
      "end-date": "",
      "output-dir": "/path/to/scratch/qc-maritimes/gistool_outputs/soil_classes",
      "lat-lims": "",
      "lon-lims": "",
      "shape-file": "/path/to/1-geofabric/merit-basins-qc-maritimes-boundaries/region_catchments.shp",
      "print-geotiff": "true",
      "stat": [
        "major"
      ],
      "quantile": "",
      "prefix": "ab_models_",
      "email": "your-email@ulaval.ca",
      "_flags": [
	"include-na",
        "submit-job",
        "parsable"
      ]
    }],

    "remap": [{
      "case-name": "ab_models",
      "cache": "/path/to/scratch/qc-maritimes/easymore_outputs/temp_dir/",
      "shapefile": "/path/to/1-geofabric/merit-basins-qc-maritimes-boundaries/region_catchments.shp",
      "shapefile-id": "COMID",
      "source-nc": "/path/to/datatool/output/directory/**/*.nc*",
      "variable-lon": "lon",
      "variable-lat": "lat",
      "variable": [
        "RDRS_v2.1_P_P0_SFC",
        "RDRS_v2.1_P_HU_1.5m",
        "RDRS_v2.1_P_TT_1.5m",
        "RDRS_v2.1_P_UVC_10m",
        "RDRS_v2.1_P_PR0_SFC",
        "RDRS_v2.1_P_FB_SFC",
        "RDRS_v2.1_P_FI_SFC"
      ],
      "remapped-var-id": "COMID",
      "remapped-dim-id": "COMID",
      "output-dir": "/path/to/qc-maritimes/easymore_outputs/remapped/",
      "_flags": [
        "submit-job"
      ]
    }]
  },
  "order": {
    "met": 1,
    "gis": -1,
    "remap": 2
  }
}
